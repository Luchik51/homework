apiVersion: v1
kind: PersistentVolume
metadata:
  name: nfs-share-fast
spec:
  capacity:
    storage: 10Gi
  accessModes:
    - ReadWriteMany
  persistentVolumeReclaimPolicy: Retain
  nfs:
    server: 10.1.4.202
    path: /ssd
---
apiVersion: v1
kind: PersistentVolume
metadata:
  name: nfs-share-normal
spec:
  capacity:
    storage: 10Gi
  accessModes:
    - ReadWriteMany
  persistentVolumeReclaimPolicy: Retain
  nfs:
    server: 10.1.4.202
    path: /ssd
